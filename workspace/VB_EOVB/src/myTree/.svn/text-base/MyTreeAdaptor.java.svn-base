package src.myTree;

import org.antlr.runtime.Token;
import org.antlr.runtime.tree.CommonTreeAdaptor;

/**
 * Deze simpele klasse zorgt ervoor dat er MyTree worden aangemaakt.
 * @author Jeroen
 *
 */
public class MyTreeAdaptor extends CommonTreeAdaptor {
	
	/**
	 * Maakt een nieuw MyTree
	 * @param t de Token
	 * @return een nieuwe MyTree
	 */
	public MyTree create(Token t){
		return new MyTree(t);
	}
}
